<h1>Listing photo_reflections</h1>

<table>
  <tr>
    <th>User</th>
    <th>Name</th>
    <th>Content type</th>
    <th>Data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photo_reflections.each do |photo_reflection| %>
  <tr>
    <td><%= photo_reflection.user_id %></td>
    <td><%= photo_reflection.name %></td>
    <td><%= photo_reflection.content_type %></td>
    <td>[Data]</td>
    <td><%= link_to 'Show', photo_reflection %></td>
    <td><%= link_to 'Edit', edit_photo_reflection_path(photo_reflection) %></td>
    <td><%= link_to 'Destroy', photo_reflection, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

