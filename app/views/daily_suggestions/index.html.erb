<h1>Listing daily_suggestions</h1>

<table>
  <tr>
    <th>User</th>
    <th>Suggestion</th>
		<th>Time Entered</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @daily_suggestions.each do |daily_suggestion| %>
  <tr>
    <td><%= daily_suggestion.user_id %></td>
    <td><%= daily_suggestion.suggestion_id %></td>
		<td><%= daily_suggestion.time_entered %></td>
    <td><%= link_to 'Show', daily_suggestion %></td>
    <td><%= link_to 'Edit', edit_daily_suggestion_path(daily_suggestion) %></td>
    <td><%= link_to 'Destroy', daily_suggestion, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Daily suggestion', new_daily_suggestion_path %>
